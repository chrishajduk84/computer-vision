                              }
                              "std":{
                                      "enabled":<True/False>
                                      "value":<value>
                                      "confidence":<value>      -> Integer value ranging from 0 to 100
                              }
                              "distribution":{                  ->Examples like gaussian/binomial/etc
                                      "enabled":<True/False>
                                      "value":<value>           
                                      "confidence":<value>      -> Integer value ranging from 0 to 100
                              }
                           }
                     
       } 
 }
